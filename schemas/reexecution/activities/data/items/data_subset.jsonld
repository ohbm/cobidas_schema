{
    "@context": "https://raw.githubusercontent.com/ReproNim/reproschema/1.0.0-rc4/contexts/generic",
    "@type": "reproschema:Field",
    "@id": "data_subset.jsonld",
    "schemaVersion": "1.0.0-rc4",
    "version": "0.0.1",
    "prefLabel": {
        "en": "data subset"
    },
    "description": "data subset",
    "ui": {
        "inputType": "radio"
    },
    "question": {
        "en": "Is the data used in this paper a subset/superset from another data collection(s)?"
    },
    "responseOptions": {
        "valueType": "xsd:integer",
        "choices": [
            {
                "name": {
                    "en": "No, neither subset or superset (the released data is the exact data for the publication)"
                },
                "value": 0
            },
            {
                "name": {
                    "en": " Yes, subset (the data used is a portion of the data available from the identified source)"
                },
                "value": 1
            },
            {
                "name": {
                    "en": " Yes, superset (the data is an aggregate from multiple sources)"
                },
                "value": 2
            }
        ],
        "minValue": 0,
        "maxValue": 2
    }
}
